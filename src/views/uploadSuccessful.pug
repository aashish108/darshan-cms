html
  include includes/head.pug
  body.page-uploadRawImagesSuccessful
    section.hero.is-fullheight
      .hero-head
        include includes/header.pug
      .hero-body
        .container.is-fluid
          .columns
            include includes/adminSidebar.pug
            .column
              h1.title= title
              h2.subtitle= message
              .columns
                if files
                  each val in files
                    .column.is-3by2
                      img.image(src=`/darshan-app/uploads/${subDir}/${val.filename}`)
      
    script(defer='' src='https://use.fontawesome.com/releases/v5.3.1/js/all.js')
    script(defer='' src='/darshan-app/public/js/uploading-raw-files.js')